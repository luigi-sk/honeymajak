<h1>Listing news_letters</h1>

<table>
  <tr>
    <th>Email</th>
  </tr>

<% @news_letters.each do |news_letter| %>
  <tr>
    <td><%=h news_letter.email %></td>
    <td><%= link_to 'Show', news_letter %></td>
    <td><%= link_to 'Edit', edit_news_letter_path(news_letter) %></td>
    <td><%= link_to 'Destroy', news_letter, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New news_letter', new_news_letter_path %>